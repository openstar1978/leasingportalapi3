        app.directive("owlCarousel", function ($timeout) {
            return {
                restrict: 'E',
                transclude: false,

                link: function (scope) {
                    scope.initCarousel = function (element,parent) {
                        console.log('initCarousel');

                        // provide any default options you want
                        var defaultOptions = {};
                        var customOptions = scope.$eval(jQuery(element).attr('data-options'));

                        // combine the two options objects
                        for (var key in customOptions) {
                            defaultOptions[key] = customOptions[key];
                        }
                        // init carousel
                        var curOwl = jQuery(element).data('owlCarousel');
                        if (!angular.isDefined(curOwl)) {

                            jQuery(element).on({

                                'initialized.owl.carousel': function () {
                                    var dataloader = scope.$eval(jQuery(element).attr('data-loader'));
                                    var dataloader2 = scope.$eval(jQuery(element).attr('data-loader2'));
                                    var dataloader3 = scope.$eval(jQuery(element).attr('data-loader3'));
                                    if (dataloader) {

                                        $timeout(function () {
                                            parent.tabloading = false;
                                        }, 1500);

                                    }
                                   else if (dataloader2) {

                                        $timeout(function () {
                                            parent.tabloading2 = false;
                                        }, 1500);
                                    }
                                    else if (dataloader3) {

                                        $timeout(function () {
                                            parent.tabloading3 = false;
                                        }, 1500);
                                    }

                                    //parent.tabloading = false;
                                    //alert(parent.tabloading);
                                }

                            }).owlCarousel(defaultOptions);

                        }
                        scope.cnt++;

                    };

                }
            };
